{
  "name": "uniswap-v3-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/graphprotocol/uniswap-v3-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "lint": "eslint . --ext .ts --fix",
    "build": "run-s codegen && graph build",
    "codegen": "graph codegen --output-dir src/types/",
    "devnet:build": "0xgraph build --network h1-devnet",
    "devnet:create": "graph create --node https://graph-api.staging.haven1.org uniswap-v3-devnet",
    "devnet:deploy": "graph deploy -l 0.0.1 --node https://graph-api.staging.haven1.org --ipfs https://ipfs.staging.haven1.org uniswap-v3-devnet",
    "devnet:remove": "graph remove --node https://graph-api.staging.haven1.org uniswap-v3-devnet",
    "devnet:graph": "NETWORK='mainnet:https://rpc.staging.haven1.org' docker-compose up",
    "testnet:build": "0xgraph build --network h1-testnet",
    "testnet:create": "graph create --node https://graph-api.staging.haven1.org uniswap-v3-testnet",
    "testnet:deploy": "graph deploy -l 0.0.1 --node https://graph-api.staging.haven1.org --ipfs https://ipfs.staging.haven1.org uniswap-v3-testnet",
    "testnet:remove": "graph remove --node https://graph-api.staging.haven1.org uniswap-v3-testnet",
    "local:graph": "NETWORK='localhost:http://host.docker.internal:8545' docker-compose up",
    "local:graph:devnet": "NETWORK='h1-devnet:https://rpc.staging.haven1.org' docker-compose up",
    "local:graph:testnet": "NETWORK='h1-testnet:https://testnet-rpc1.haven1.org' docker-compose up",
    "local:build": "yarn build --network localhost",
    "local:create": "0xgraph create --node http://localhost:8020 uniswap-v3-local",
    "local:deploy": "0xgraph deploy -l 0.0.1 --node http://localhost:8020 --ipfs http://localhost:5001 uniswap-v3-local",
    "local:graph:clean": "docker-compose down -v && docker-compose rm -v && rm -rf data/ipfs data/postgres",
    "local:remove": "0xgraph remove --node http://localhost:8020 uniswap-v3-local",
    "local:setup": "yarn codegen && yarn build && yarn local:create && yarn local:deploy",
    "local:watch": "graph deploy uniswap-v3-local --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001",
    "build:docker": "docker build -t matchstick .",
    "buildonly": "graph build",
    "test": "graph test -d",
    "test:no-tty": "docker run -i --rm --mount type=bind,source=$INIT_CWD,target=/matchstick matchstick"
  },
  "devDependencies": {
    "@0xgraph/cli": "^0.2.1",
    "@graphprotocol/graph-cli": "^0.91.1",
    "@graphprotocol/graph-ts": "^0.36.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@uniswap/eslint-config": "^1.2.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "matchstick-as": "^0.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  }
}
